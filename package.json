    "@vitest/coverage-v8": "3.0.7",
    "axios-mock-adapter": "2.1.0",
    "dotenv": "16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "tsx": "4.8.0",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.26.0",
    "vitest": "^3.0.7"
  },
  "imports": {
    "#database": [
      "./build/database/index.js"
    ],
