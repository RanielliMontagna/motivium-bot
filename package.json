{
  "name": "motivium-bot",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "check": "tsc --noEmit && echo âœ” Ok",
    "build": "tsc",
    "dev": "tsx --env-file .env ./src/index.ts",
    "dev:dev": "tsx --env-file .env.dev ./src/index.ts",
    "watch": "tsx --watch --env-file .env ./src/index.ts",
    "watch:dev": "tsx --watch --env-file .env.dev ./src/index.ts",
    "start": "node --env-file .env .",
    "start:dev": "node --env-file .env.dev .",
    "start:migrate:prod": "prisma migrate deploy && node build/index.js",
    "prestart": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@magicyan/discord": "1.3.1",
    "@prisma/client": "6.5.0",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.12.2",
    "chalk": "5.4.1",
    "dayjs": "^1.11.13",
    "discord.js": "14.18.0",
    "fast-glob": "3.3.3",
    "handlebars": "4.7.8",
    "he": "1.2.0",
    "node-cache": "5.1.2",
    "node-cron": "^3.0.3",
    "node-html-to-image": "5.0.0",
    "openai": "4.90.0",
    "puppeteer": "24.23.0",
    "rou3": "0.5.1",
    "rss-parser": "3.13.0",
    "telegram": "2.26.22",
    "vite": "7.1.9",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/he": "1.2.3",
    "@types/node": "22.13.14",
    "@vitest/coverage-v8": "3.2.4",
    "axios-mock-adapter": "2.1.0",
    "dotenv": "16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prisma": "6.5.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "^3.2.4"
  },
  "imports": {
    "#database": [
      "./build/database/index.js"
    ],
    "#base": [
      "./build/discord/base/index.js"
    ],
    "#schedulers": [
      "./build/discord/schedulers/index.js"
    ],
    "#services": [
      "./build/services/index.js"
    ],
    "#libs": [
      "./build/libs/index.js"
    ],
    "#utils": [
      "./build/discord/utils/index.js"
    ],
    "#functions": [
      "./build/functions/index.js"
    ],
    "#menus": [
      "./build/menus/index.js"
    ],
    "#settings": [
      "./build/settings/index.js"
    ],
    "#tools": [
      "./build/tools/index.js"
    ],
    "#server": [
      "./build/server/index.js"
    ],
    "#lib": [
      "./build/lib/index.js"
    ]
  }
}
